{
  "compilerOptions": {
    "jsx": "react",
"target": "ES2015",
"module": "ES2020",
    "rootDirs": [
      ".",
      "./src/backend"
    ],
    "allowImportingTsExtensions": true,
    "allowJs": false,
    "sourceMap": true,
    "outDir": "./dist",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/backend",
    "**/*.js",
    "**/*.jsx"
  ],
  "exclude": [
    "node_modules",
    "directory",
    "archive",
    "**/result"
  ],
  "files": [
    "src/index.tsx",
    
  ]
}
